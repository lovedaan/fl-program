<view class="index-page">
  <view class="banner-wrap">
    <swiper class="banner-swiper" indicator-dots="{{indicatorDots}}"
        autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" circular="{{circular}}">
        <block wx:for="{{background}}" wx:key="*this">
          <swiper-item>
            <image mode="aspectFill" src="{{item}}"></image>
          </swiper-item>
        </block>
      </swiper>
  </view>
  <view class="nav-wrap">
    <view class="line-title"><text>一切尽在掌握</text></view>
    <view class="nav-box">
      <navigator hover-class="none" url="/pages/query1/index" class="nav-item"><text>认证查询</text></navigator>
      <navigator hover-class="none" url="/pages/query2/index" class="nav-item"><text>授权培训点查询</text></navigator>
    </view>
  </view>
  <view class="map">
    <map longitude="{{longitude}}" latitude="{{latitude}}" />
  </view>
</view>
